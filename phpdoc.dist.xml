<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
	<parser>
		<target>documentation</target>
	</parser>
	<transformer>
		<target>documentation</target>
		<template></template>
	</transformer>
	<transformations>
        <template name="responsive-twig" />
    </transformations>
	<files>
		<directory>.</directory>
		<ignore>cache/*</ignore>
		<ignore>run/*</ignore>
		<ignore>schema/*</ignore>
		<ignore>tests/*</ignore>
	</files>
</phpdoc>